<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.womentechmakers.wtmapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_womentechies"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"

        android:theme="@style/AppTheme">

        <activity
            android:name="com.womentechmakers.wtmapp.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.womentechmakers.wtmapp.About.AboutWTMActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name="com.womentechmakers.wtmapp.About.AboutDSCActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>

        <activity
            android:name="com.womentechmakers.wtmapp.Quiz.QuizActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>

    </application>

</manifest>